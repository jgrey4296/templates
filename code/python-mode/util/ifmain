def exit_handler():
    print("Exiting")
    return

async def async_init ():
      pass

def main():
    atexit.register(exit_handler)
    with AsyncRunner() as runner:
         runner.run(async_init())

##-- ifmain
if __name__ == "__main__":
    main()
##-- end ifmain
